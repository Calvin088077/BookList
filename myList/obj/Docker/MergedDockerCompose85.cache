name: dockercompose8825136332199134999
services:
  db:
    environment:
      MYSQL_DATABASE: list
      MYSQL_ROOT_PASSWORD: calvin0701
    image: mysql:9.1.0
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "3307"
        protocol: tcp
    volumes:
      - type: volume
        source: mysql_data
        target: /var/lib/mysql
        volume: {}
  mylist:
    build:
      context: C:\Users\user\source\repos\myList
      dockerfile: myList/Dockerfile
    depends_on:
      db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      Server: db;Database=list;User=root;Password=calvin0701
    image: mylist
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "5000"
        protocol: tcp
networks:
  default:
    name: dockercompose8825136332199134999_default
volumes:
  mysql_data:
    name: dockercompose8825136332199134999_mysql_data